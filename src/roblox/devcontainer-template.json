{
    "id": "roblox",
    "version": "0.2.0",
    "name": "Roblox",
    "description": "Develop on Roblox using Rojo. Includes a toolchain manager, extensions, CLI, and dependencies.",
    "documentationURL": "https://github.com/ryanlua/templates/tree/main/src/roblox",
    "publisher": "Ryan Luu",
    "licenseURL": "https://github.com/ryanlua/templates/blob/main/LICENSE",
    "options": {
        "rojoVersion": {
            "type": "string",
            "description": "Select a Rojo version to install",
            "proposals": [
                "latest",
                "7.4.1",
                "7.4.0",
                "7.3.0",
                "7.2.1",
                "7.1.1",
                "7.0.0",
                "6.2.0",
                "6.1.0",
                "6.0.2",
                "0.5.4",
                "0.4.13",
                "0.3.2",
                "0.2.3",
                "0.1.0"
            ],
            "default": "latest"
        },
        "toolchainManager": {
            "type": "string",
            "enum": [
                "aftman",
                "foreman",
                "rokit",
                "none"
            ],
            "default": "rokit",
            "description": "Select a toolchain manager to install."
        }
    },
    "platforms": [
        "Roblox",
        "Luau"
    ],
    "keywords": [
        "Rokit",
        "Aftman",
        "Foreman",
        "Roblox",
        "Luau",
        "Rojo"
    ]
}
